<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>MVCDAOforServletToMysql</display-name>

  <servlet>
    <servlet-name>MVC</servlet-name>
    <servlet-class>com.js.ControllerServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>MVC</servlet-name>
    <url-pattern>/connurl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>all</servlet-name>
    <servlet-class>com.js.AllDetailController</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
  
    <servlet-name>all</servlet-name>
    <url-pattern>/allurl</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>wel</servlet-name>
    <servlet-class>com.js.WelcomeServlet</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>wel</servlet-name>
    <url-pattern>/WelcomeServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>insert</servlet-name>
    <servlet-class>com.js.InsertStudentInMysql</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>insert</servlet-name>
    <url-pattern>/inserturl</url-pattern>
  </servlet-mapping>
  
</web-app>